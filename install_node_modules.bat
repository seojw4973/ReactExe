@echo off
setlocal enabledelayedexpansion

REM 프로젝트 디렉터리가 있는 상위 디렉터리 설정
set "BASE_DIR=D:\Lecture\ReactExe"

REM 각 프로젝트 디렉터리 순회
for /d %%i in ("%BASE_DIR%\*") do (
    if exist "%%i\package.json" (
        echo Installing node modules in %%i
        cd "%%i"
        npm install
        cd ..
    )
)

echo Node modules installation completed!
pause
